# Azure RAGcelerator - Environment Variables
# Copy this file to .env and fill in your values

# Azure Storage
STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=<your-storage-account>;AccountKey=<your-key>;EndpointSuffix=core.windows.net
DOCUMENTS_CONTAINER_NAME=documents

# Azure Cognitive Search
SEARCH_ENDPOINT=https://<your-search-service>.search.windows.net
SEARCH_API_KEY=<your-search-admin-key>
SEARCH_INDEX_NAME=rag-documents

# Azure OpenAI
OPENAI_ENDPOINT=https://<your-openai-resource>.openai.azure.com
OPENAI_API_KEY=<your-openai-key>
OPENAI_API_VERSION=2024-02-01
EMBEDDING_MODEL=text-embedding-ada-002
CHAT_MODEL=gpt-35-turbo

# Processing Settings (optional)
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
EMBEDDING_BATCH_SIZE=16

# UI Settings (optional)
MAX_SEARCH_RESULTS=10
MAX_CONTEXT_CHUNKS=5

# Application Insights (optional)
APPLICATIONINSIGHTS_CONNECTION_STRING=



